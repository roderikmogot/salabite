// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Daily {
  id Int @id @default(autoincrement())
  date String
  dailyCases String
}

model Global {
  id Int @id @default(autoincrement())
  date String
  confirmed String
  recovered String
  deaths String
}